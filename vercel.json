{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "cd apps/web && pnpm run build",
  "outputDirectory": "apps/web/.next"
}